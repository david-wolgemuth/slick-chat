{
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console": [
            "warn"
        ],
        "semi": [
            2,
            "always"
        ],
        no-unused-vars: [
            "warn"
        ]
    },
    "parserOptions": {
        "sourceType": "module"
    }
}